    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.20",
    "node-zip-dir": "^1.0.2",
    "sharp": "^0.34.3",
    "tailwindcss": "^4.0.0-alpha.30",
    "typescript": "^5.5.4",
    "vite": "^5.4.0"
